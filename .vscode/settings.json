{
	"editor.unusualLineTerminators": "off",
	"editor.guides.highlightActiveBracketPair": false,
	"editor.guides.highlightActiveIndentation": false,
	"editor.occurrencesHighlight": false,
	"editor.renderLineHighlight": "none",
	"editor.selectionHighlight": false,
	"editor.copyWithSyntaxHighlighting": false,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"editor.unicodeHighlight.includeComments": false,
	"editor.unicodeHighlight.nonBasicASCII": false,
	"editor.unicodeHighlight.invisibleCharacters": false,
	"editor.matchBrackets": "never",
	"editor.foldingHighlight": false,
	"editor.bracketPairColorization.enabled": false,
	"editor.emptySelectionClipboard": false,
	"editor.lightbulb.enabled": false,
	"editor.unicodeHighlight.includeStrings": false,
	"diffEditor.renderMarginRevertIcon": false,
	"diffEditor.renderSideBySide": true,
	"diffEditor.renderIndicators": false,
	"editor.minimap.renderCharacters": false,
	"editor.renderControlCharacters": false,
	"editor.guides.indentation": false,
	"editor.renderWhitespace": "none",
	"scm.countBadge": "off",
	"git.countBadge": "off",
	"breadcrumbs.showFiles": false,
	"editor.tabSize": 2,
	"editor.insertSpaces": false,
	"editor.suggest.showFiles": true,
	"git.promptToSaveFilesBeforeCommit": "never",
	"git.promptToSaveFilesBeforeStash": "never",
	"git.openRepositoryInParentFolders": "never",
	"prettier.configPath": ".prettierrc",
	"editor.experimentalWhitespaceRendering": "off",
	"editor.renderFinalNewline": "off",
	"scm.diffDecorationsGutterPattern": {
		"modified": false
	},
	"scm.diffDecorationsGutterAction": "none",
	"editor.showFoldingControls": "never",
	"editor.snippets.codeActions.enabled": true,
	"tailwindCSS.codeActions": true,
	"editor.codeActionWidget.showHeaders": true,
	"git.showProgress": false,
	"editor.comments.ignoreEmptyLines": true,
	"editor.experimental.pasteActions.enabled": true,
	"editor.suggest.showFolders": false,
	"editor.detectIndentation": false
}